<Window x:Class="EmbeddedWebSampleApps.CefTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmbeddedWebSampleApps.CefTester"
        xmlns:cef="clr-namespace:Xilium.CefGlue.WPF;assembly=Xilium.CefGlue.WPF"
        xmlns:common="clr-namespace:EmbeddedWebSampleApps.Common;assembly=EmbeddedWebSampleApps.Common"
        mc:Ignorable="d"
        Title="EmbeddedWebSampleApps.CefTester"
        Height="768"
        Width="1366">
    <DockPanel>
        <StackPanel DockPanel.Dock="Right">
            <common:PerformanceControl />
        </StackPanel>
        <cef:WpfCefBrowser Name="cefBrowser"
                           Address="https://browserbench.org/Speedometer3.0" />
    </DockPanel>
</Window>