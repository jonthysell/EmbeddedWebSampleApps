<Window x:Class="EmbeddedWebSampleApps.WebView2Tester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmbeddedWebSampleApps.WebView2Tester"
        xmlns:common="clr-namespace:EmbeddedWebSampleApps.Common;assembly=EmbeddedWebSampleApps.Common"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        mc:Ignorable="d"
        Title="EmbeddedWebSampleApps.WebView2Tester"
        Height="768"
        Width="1366">
    <DockPanel>
        <StackPanel DockPanel.Dock="Right">
            <common:PerformanceControl />
        </StackPanel>
        <wv2:WebView2 Name="webView"
                      Source="https://browserbench.org/Speedometer3.0" />
    </DockPanel>
</Window>